{
 "AWSEBDockerrunVersion": 2,
 "containerDefinitions": [
  {
    "name": "client",
    "image": "supattra/multi-client",
    "hostname": "client",
    "essential": false
  },
  {
    "name": "server",
    "image": "supattra/multi-server",
    "hostname": "api",
    "essential": false
  },
  {
    "name": "worker",
    "image": "supattra/multi-worker",
    "hostname": "worker",
    "essential": false
  },
  {
    "name": "nginx",
    "image": "supattra/multi-nginx",
    "essential": true,
    "portMappings": [
     {
      "hostPort": 80,
      "containerPort": 80
     }
    ],
    "links": [
     "client", "server"
    ]
  }
 ] 
}
